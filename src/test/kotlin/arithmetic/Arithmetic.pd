pi main() : Int {
    let a : Int = 1;
    // 2
    let b : Int = a + 1;
    // 3
    let c : Int = a + b;
    // 6
    let d : Int = a + b + c;
    // 13
    let e : Int = 1 + 2 * d;
    // 30
    let f : Int = 1 + 2 * e + 3;
    // 15
    let g : Int = f / 2;
    // 9
    let h : Int = 2 + g / 2;

    (h) |> print;
    >> 0;
}